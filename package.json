{
  "name": "path-prop",
  "repository": "github:fabiospampinato/path-prop",
  "description": "Fast library for manipulating plain objects using paths.",
  "version": "2.0.0",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "benchmark": "tsex benchmark",
