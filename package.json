{
  "name": "path-prop",
  "description": "Fast library for manipulating plain objects using paths.",
  "version": "1.1.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "benchmark": "node tasks/benchmark.js",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage .nyc_output",
    "clean": "npm run clean:dist && npm run clean:coverage",
    "compile": "tsc --skipLibCheck",
    "compile:watch": "tsc --skipLibCheck --watch",
    "test": "ava",
    "test:watch": "ava --watch",
    "coverage": "nyc --reporter=html ava",
    "report": "nyc report",
    "report:html": "open coverage/index.html",
    "prepublishOnly": "npm run clean && npm run compile && npm run coverage"
  },
  "ava": {
    "files": [
      "test/index.js"
    ]
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/path-prop/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/path-prop.git"
  },
  "keywords": [
    "object",
    "path",
    "prop",
    "get",
    "set",
    "delete",
    "remove",
    "has",
    "flat",
    "unflat"
  ],
  "dependencies": {
    "plain-object-is-empty": "^1.0.0",
    "plain-object-merge": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^12.7.11",
    "ava": "^2.4.0",
    "ava-spec": "^1.1.1",
    "benchloop": "^1.3.1",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "typescript": "^3.6.3"
  }
}
